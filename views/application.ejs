<body class="full-width">
  <header class="bt">
    <div class="new-container">
      <div class="flex-box">
        <div class="logo-box">
          <a href=""><img src="/img/logo.svg" alt="" /></a>
        </div>
        <div class="new-btn-box">
          <a href="" class="dk"><img src="/img/phone-phn.png" alt="" />Call us at
            1-800-413-3220</a>
          <a href="" class="mobile-btn"><img src="/img/callbtn.png" alt="" /></a>
        </div>
      </div>
    </div>
  </header>

  <div class="banner-section form-banner">
    <div class="new-container">
      <div class="heading-box">
        <h1>Small Business Lending Reimagined</h1>
        <h5>Simple Smart Technology for Better Loans</h5>
      </div>
      <div class="white-form-section">
        <form action="create-account" id="regForm" enctype="multipart/form-data" method="POST">
          <div class="loading">
            <div class='uil-ring-css' style='transform:scale(0.79);'>
              <div></div>
            </div>
          </div>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Enter Your Full Name</h3>
            <input type="text" name="fullName" id="fullnameInput" placeholder="Full name" />
            <h5 style="color: red; display: none" id="nameinputerror"></h5>
            <p>
              Just answer a few questions to see your options. It’s free &
              won’t impact your credit score.
            </p>
            <span class="errormsg"></span>
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Enter Your Company Name</h3>
            <input type="text" name="cmpName" placeholder="Company name" />
            <h5 style="color: red; display: none"></h5>
            
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Select Your Business Industry</h3>

            <select name="industry" id="industryselect">
              <option value="" selected>Industry</option>
              <option value="Agriculture">Agriculture</option>
              <option value="Automobile Repair">Automobile Repair</option>
              <option value="Services">Services</option>
              <option value="Chiropractic Treatment">Chiropractic Treatment</option>
              <option value="Computer/Electronics">Computer/Electronics</option>
              <option value="Consumer Services">Consumer Services</option>
              <option value="Construction">Construction</option>
              <option value="Education">Education</option>
              <option value="Energy">Energy</option>
              <option value="Financial Services">Financial Services</option>
              <option value="Food Service">Food Service</option>
              <option value="Government">Government</option>
              <option value="Gym">Gym</option>
              <option value="Health/Pharmaceuticals">Health/Pharmaceuticals</option>
              <option value="Hospitality">Hospitality</option>
              <option value="Manufacturing">Manufacturing</option>
              <option value="Mining">Mining</option>
              <option value="Media/Entertainment">Media/Entertainment</option>
              <option value="Non-Profit">Non-Profit</option>
              <option value="Real Estate">Real Estate</option>
              <option value="Retail">Retail</option>
              <option value="Software/Internet">Software/Internet</option>
              <option value="Telecommunications">Telecommunications</option>
              <option value="Transportation/Storage">Transportation/Storage</option>
              <option value="Travel/Leisure">Travel/Leisure</option>
              <option value="Utilities">Utilities</option>
              <option value="Wholesale/Distribution">Wholesale/Distribution</option>
              <option value="Other">Other</option>
            </select>

            <h5 style="color: red; display: none"></h5>
            
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Select Your Entity Type</h3>

            <select name="cmpType" required>
              <option value="">Entity Type</option>
              <option value="sole proprietor">sole proprietor</option>
              <option value="limited liability company ( LLC)">limited liability company ( LLC)</option>
              <option value="Corporation">Corporation</option>
              <option value="General Partnership">General Partnership</option>
              <!-- <option value="Limited Partnership ( LP )">Limited Partnership ( LP )</option>
              <option value="Limited Liability Partnership ( LP )">Limited Liability Partnership ( LP )</option> -->
              <!-- <option value="Non-Profit">Non-Profit</option> -->
            </select>

            <h5 style="color:red;display:none" id="cmpTypeval">  </h5>
 
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Enter Your Business Start Date</h3>
            <!-- <div>
                <input type="date" name="startDate" /> -->

            <div class="flex-box">

              <input type="number" name="startDate" class="sm businessstartdate" maxlength="1" placeholder="M"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="number" name="startDate" placeholder="M" class="sm businessstartdate businessstartdate2"
                maxlength="1"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="number" name="startDate" placeholder="Y" class="sm businessstartdate businessstartdate2"
                maxlength="1"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="number" name="startDate" placeholder="Y" class="sm businessstartdate businessstartdate2"
                maxlength="1"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="number" name="startDate" placeholder="Y" class="sm businessstartdate businessstartdate2"
                maxlength="1"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="number" name="startDate" placeholder="Y" class="sm businessstartdate" maxlength="1"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

            </div>
            <h5 style="color: red; display: none"></h5> 
          </fieldset>


          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Enter Your Business Zip Code</h3>


            <div class="flex-box space">

              <input type="text" placeholder="" class="col-3 zipcodeinputfield" name="zipCode" maxlength="1"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="text" placeholder="" class="col-3 zipcodeinputfield" maxlength="1" name="zipCode"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="text" placeholder="" class="col-3 zipcodeinputfield" maxlength="1" name="zipCode"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="text" placeholder="" class="col-3 zipcodeinputfield" maxlength="1" name="zipCode"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />

              <input type="text" placeholder="" class="col-3 zipcodeinputfield" maxlength="1" name="zipCode"
                oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" />            
            </div>

            <div>
              <input type="hidden" name="zipcity" id="zipcity">
              <input type="hidden" name="zipstate" id="zipstate">
            </div>

            <div class="msg-box">
              <h4 id="shoecityofzip" style=""></h4>
              <h5 id="shoecityofziperror" style="color: red; display: none"></h5>
            </div> 
          </fieldset>



          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Enter your Request Loan Amount</h3>
            <div class="dollar-box">
              <input type="text" name="loanAmount" id="loanAmount" placeholder="$"  />
            </div>
            <h5 style="color: red; display: none"></h5> 
          </fieldset>

          <fieldset class="tab ">
            <div class="bar"></div>
            <h3>Enter your Annual Revenue</h3>
            <div class="dollar-box">
              <input type="text" name="annualRevenue" id="annualRevenuevalue" placeholder="$" />
            </div>
            <h5 style="color: red; display: none"></h5>
             
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Select Your Credit Score</h3>
            <div class="selector">

              <div class="selecotr-item">
                <input type="radio" id="radio1" value="Poor (639 or less)" name="creditScore"
                  class="selector-item_radio" />
                <label for="radio1" class="selector-item_label">Poor (639 or less)</label>
              </div>

              <div class="selecotr-item">
                <input type="radio" id="radio2" value="Fair (640-679)" name="creditScore" class="selector-item_radio" />
                <label for="radio2" class="selector-item_label">Fair (640-679)</label>
              </div>

              <div class="selecotr-item">
                <input type="radio" id="radio3" name="creditScore" value="Good (680-719)" class="selector-item_radio" />
                <label for="radio3" class="selector-item_label">Good (680-719)</label>
              </div>

              <div class="selecotr-item">
                <input type="radio" id="radio4" name="creditScore" value="720+" class="selector-item_radio" />
                <label for="radio4" class="selector-item_label">Excellent (720+)</label>
              </div>

            </div>

            <h5 style="color: red; display: none"></h5>
            
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Select your Purpose of Loan</h3>
            <!-- <input name="purposeOfLone" /> -->

            <select name="purposeOfLone" id="">
              <option value="">Purpose of Loan</option>
              <option value="Buy An Existing Business">Buy An Existing Business</option>
              <option value="Expansion">Expansion</option>
              <option value="Payroll">Payroll</option>
              <option value="Purchasing Equipment">Purchasing Equipment</option>
              <option value="Working Capital">Working Capital</option>
              <option value="Other">Other</option>
              <option value="Real Estate">Real Estate</option>
            </select>

            <h5 style="color: red; display: none" id="purposeOfLoneval"></h5>
          
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Enter your Phone Number</h3>
            <!-- <input type="number" name="phone" placeholder="(+1)" maxlength="10"
              oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);" /> -->


              <!-- <input type="tel" name="phone" id="phoneW" pattern="(?:\(\d{3}\)|\d{3})[- ]?\d{3}[- ]?\d{4}" maxlength="14" placeholder="(xxx) xxx-xxxx" required /> -->
              <input type="tel" name="phone" id="myInput" oninput="phoneformater()" placeholder="+1 (###) ### - ####" required pattern="\([0-9]{3}\) [0-9]{3} - [0-9]{4}">
            <h5 style="color: red; display: none"></h5>
            
          </fieldset>

          <fieldset class="tab ">
            <div class="bar"></div>
            <div class="bank-box">
              <h3 >Please authenticate your bank accounts for loan processing & related documents</h3>
              <button type="button" class="action-button" id="link-button" >
                Connect & verify your bank account
              </button>
            </div>
            <input type="hidden" name="hidden_public_token" id="hidden_public_token" />
            <input type="hidden" name="hidden_address" id="hidden_address" />

            <h5 style="color: red; display: none; text-align: center;"></h5>
            <ul class="flex-box bank-list">
              <li>
                <img src="/img/Group-1762.png" alt="">
                <h6><strong>Advanced Encryption </strong>Standard <br>(AES-256) and  (TLS)</h6>
              </li>
              <li>
                <img src="/img/image-47.png" alt="">
                <h6><strong>Secure cloud<br>
                  infrastructure </strong>technologies</h6>
              </li>
              <li>
                <img src="/img/image-48.png" alt="">
                <h6><strong>Security audits</strong>  by researchers<br>
                  and financial institutions
                  </h6>
              </li>
            </ul>
          </fieldset>


          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Upload Your Driving License</h3>
            <div class="file-box">
              <input type="file" name="drivinLicense" id="drivinLicense" placeholder="Choose Your File" class="file"
                style="text-indent: -9999px;" />
            </div>

            <div class="msg-box">
              <h4 id="shoedrivinglicense" style=""></h4> 
            </div>
            <h5 style="color: red; display: none"></h5>
 
          </fieldset>

          <!-- <fieldset class="tab">
            <div class="bar"></div>
            <h3>Upload Previous 3 Months Bank Statements</h3>
            <div class="file-box">
              <input type="file" id="bankStatemets" name="bankStatemets" placeholder="Choose Your File" class="file"
                style="text-indent: -9999px;" required />
            </div>
            <h5 style="color: red; display: none"></h5>
            <p>Following file type is applicable in .jpeg .png or pdf</p>
          </fieldset> -->

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Upload Your Voided Check</h3>
            <div class="file-box">
              <input type="file" name="voided" id="voided" placeholder="Choose Your File" class="file"
                style="text-indent: -9999px;" />
            </div>
            <div class="msg-box">
              <h4 id="shoevoided" style=""></h4> 
            </div>
            <h5 style="color: red; display: none"></h5>
 
          </fieldset>

          <fieldset class="encryption tab">
            <div class="bar"></div>
            <h3>Enter Your SSN (243 bit encryption)</h3>
            <input type="text" name="ssn" placeholder="Enter Your Number" onkeydown="ssnFormatter()" id="ssninputbox" />
            <h5 style="color: red; display: none"></h5>
           
            <img src="/img/bit-icon.png" alt="" class="mobile-img" />
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Enter Your email</h3>
            <input type="text" name="email" placeholder="Type your email" />
            <h5 style="color: red; display: none"></h5>
          </fieldset>

          <fieldset class="tab">
            <div class="bar"></div>
            <h3>Enter Your Tax ID #</h3>
            <input type="text" name="taxId" placeholder="Type your ID" onkeydown="taxidFormatter()"
              id="taxidinputbox" />
            <h5 style="color: red; display: none"></h5>
          </fieldset>
          <!-- type="submit" -->
          <div class="button-box flex-box">
              <button type="button" class="next prev-button" id="prevBtn" onclick="nextPrev(-1)"
              style="">
              Previous
            </button>
            <button type="button" class="next action-button" id="nextBtn" onclick="nextPrev(1)">
              next
            </button>
          </div>

        </form>
        <div class="thanks-box active" id="thanks-box1">
          <!-- <div id="showsecond" style="display: none;"> -->
          <img src="/img/thanks.png" alt="" />
          <h4>
            <strong>Thanks for your submission</strong> and we will get back
            to you with best offer
          </h4>
         <!-- </div> -->
          <!-- <div id="showfirst" style="display: none;"> 
            <div id="thanks-animation" >
              <lottie-player src="https://assets6.lottiefiles.com/packages/lf20_ofuo42yn.json"  background="transparent"  speed="1"  style="width: 100%; height: 100%;" hover loop  autoplay></lottie-player>
            </div>
          </div>       -->
        </div>
      </div>
    </div>
   <div class="mobile-button">
    <button type="button" class="next prev-button mobile-btn" id="prevBtn" onclick="nextPrev(-1)"
    style="">
    Previous
  </button>
    <button type="submit" class="next action-button mobile-btn" id="nextBtn" onclick="nextPrev(1)">
